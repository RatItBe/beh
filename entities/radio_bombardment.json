{
    "format_version": "1.16.230",
    "minecraft:entity": {
        "description": {
            "identifier": "fs:radio_bombardment",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "despawn": {
                "minecraft:instant_despawn": {}
            },
            "roar": {
                "minecraft:behavior.knockback_roar": {
                    "priority": 0,
                    "duration": 1,
                    "attack_time": 0.1,
                    "knockback_damage": 30,
                    "knockback_strength": 1,
                    "knockback_range": 7,
                    "knockback_filters": {
                        "test": "is_family",
                        "subject": "other",
                        "operator": "==",
                        "value": "monster"
                    },
                    "damage_filters": {
                        "test": "is_family",
                        "subject": "other",
                        "operator": "==",
                        "value": "monster"
                    },
                    "on_roar_end": {
                        "event": "kill",
                        "target": "self"
                    },
                    "cooldown_time": 0
                }
            }
        },
        "components": {
            "minecraft:timer": {
                "looping": true,
                "time_down_event": {
                    "event": "kill",
                    "target": "self"
                },
                "time": 7
            },
            "minecraft:projectile": {
                "on_hit": {
                    "impact_damage": {
                        "damage": 1,
                        "knockback": true,
                        "destroy_on_hit": false,
                        "semi_random_diff_damage": false,
                        "max_critical_damage": 30,
                        "min_critical_damage": 20
                    },
                    "stick_in_ground": {
                        "shake_time": 0.35
                    },
                    "definition_event": {
                        "affectProjectile": true,
                        "eventTrigger": {
                            "event": "boom",
                            "target": "self"
                        }
                    }
                },
                "power": 10,
                "gravity": 0.0,
                "uncertaintyBase": 20,
                "inertia": 3,
                "uncertaintyMultiplier": 0,
                "anchor": 1,
                "offset": [
                    0,
                    -0.1,
                    0
                ],
                "reflectOnHurt": false,
                "multiple_targets": true,
                "shoot_sound": "random.explode",
                "ignored_entities": [
                    "player",
                    "fs:radio_bombardment",
                    "fs:radio_signal_dummy"
                ]
            },
            "minecraft:target_nearby_sensor": {
                "inside_range": 10,
                "outside_range": 10.1,
                "must_see": true,
                "on_inside_range": {
                    "event": "boom",
                    "target": "self"
                },
                "on_outside_range": {}
            },
            "minecraft:environment_sensor": {
                "triggers": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "on_ground",
                                    "subject": "self"
                                }
                            ]
                        },
                        "event": "roar",
                        "target": "self"
                    }
                ]
            },
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 0.6
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": false
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 80.0,
                    "max_dropped_ticks": 10,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:despawn": {
                "despawn_from_simulation_edge": true
            }
        },
        "events": {
            "boom": {
                "add": {
                    "component_groups": [
                        "roar"
                    ]
                },
                "queue_command": {
                    "command": [
                        "execute @s ~~~ tp @s ~~~",
                        "playsound random.explode @a[r=100]"
                    ],
                    "target": "self"
                }
            },
            "despawn": {
                "add": {
                    "component_groups": [
                        "despawn"
                    ]
                }
            },
            "kill": {
                "add": {
                    "component_groups": [
                        "despawn"
                    ]
                }
            }
        }
